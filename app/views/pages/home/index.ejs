  <script>
    $('.slider-for').slick({
      slidesToShow: 1,
      slidesToScroll: 1,
      arrows: false,
      fade: true,
      asNavFor: '.slider-parceiras'
    });

    $('.slider-parceiras').slick({
      asNavFor: '.slider-for',
      arrows: false,
      speed: 300,
      autoplay: true,
      autoplaySpeed: 5000,
      focusOnSelect: true,
      centerMode: true,
      centerPadding: '60px',
      variableWidth: true,
      slidesToShow: 3,
      responsive: [
        {
          breakpoint: 768,
          settings: {
            arrows: false,
            centerMode: true,
            centerPadding: '40px',
            slidesToShow: 3,
            variableWidth: false,
          }
        },
        {
          breakpoint: 480,
          settings: {
            arrows: false,
            centerMode: true,
            centerPadding: '0px',
            slidesToShow: 1,
            variableWidth: true,
          }
        }
      ]
    });
  </script>
