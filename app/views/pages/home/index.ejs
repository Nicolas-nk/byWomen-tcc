<body>
  <div class="container">
    <header>
      <div class="navbar bg-purplegradient">
        <nav class="header-navbar">
          <div class="header-components">
            <img src="/images/logo/logo.svg" alt="" />
          </div>
          <div class="header-components">
            <a href=""><img src="/images/icons/header/lupa.svg" alt="" /></a>
            <a href="/login"><img src="/images/icons/header/usuario.svg" alt="" /></a>
          </div>
        </nav>
      </div>
    </header>
    <main>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="/images/banners/torne-seColaboradora.png" class="carousel-itens" alt="..." usemap="#image-map" />

          <map name="image-map">
            <area target="_self" alt="torne-se colaboradora" title="torne-se colaboradora" href="..."
