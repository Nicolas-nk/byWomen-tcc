    ></script>
    <script>
      $(".slider-for").slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: false,
        fade: true,
        asNavFor: ".slider-parceiras",
      });

      $(".slider-parceiras").slick({
        asNavFor: ".slider-for",
        arrows: false,
        speed: 300,
        autoplay: true,
        autoplaySpeed: 5000,
        focusOnSelect: true,
        centerMode: true,
        centerPadding: "60px",
        variableWidth: true,
        slidesToShow: 3,
        responsive: [
          {
            breakpoint: 768,
            settings: {
              arrows: false,
              centerMode: true,
              centerPadding: "40px",
              slidesToShow: 3,
              variableWidth: false,
            },
          },
          {
            breakpoint: 480,
            settings: {
              arrows: false,
              centerMode: true,
              centerPadding: "0px",
              slidesToShow: 1,
              variableWidth: true,
            },
          },
        ],
      });
    </script>
    <script>
      $(document).ready(function () {
        $("body").css("overflow-y", "hidden");

        $(".oportunidade").show();
        $(".proposito").hide();
        $(".escolha-especialistas").hide();

        $(".inf-bw").show();

        $(".skip").click(function () {
          $(".inf-bw").hide();
          $("body").css("overflow-y", "unset");
        });

        $(".slide1").click(function () {
          $(".slide1").addClass("checked");
          $(".slide2").removeClass("checked");
          $(".slide3").removeClass("checked");
          $(".slide-inf").addClass("image-inf");
          $(".slide-inf").removeClass("image-inf2");
          $(".slide-inf").removeClass("image-inf3");
          $(".oportunidade").show();
          $(".proposito").hide();
          $(".escolha-especialistas").hide();
        });
        $(".slide2").click(function () {
          $(".slide2").addClass("checked");
          $(".slide1").removeClass("checked");
          $(".slide3").removeClass("checked");
          $(".slide-inf").addClass("image-inf2");
          $(".slide-inf").removeClass("image-inf");
          $(".slide-inf").removeClass("image-inf3");
          $(".escolha-especialistas").show();
          $(".proposito").hide();
          $(".oportunidade").hide();
        });
        $(".slide3").click(function () {
          $(".slide3").addClass("checked");
          $(".slide2").removeClass("checked");
          $(".slide1").removeClass("checked");
          $(".slide-inf").addClass("image-inf3");
          $(".slide-inf").removeClass("image-inf2");
          $(".slide-inf").removeClass("image-inf");
          $(".proposito").show();
          $(".escolha-especialistas").hide();
          $(".oportunidade").hide();
        });
      });
    </script>
